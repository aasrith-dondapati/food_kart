{
    "projects": [
      {
        "src": "./client",
        "buildCommand": "npm install && npm run build",
        "outputDirectory": "client/dist"
      },
      {
        "src": "./server",
        "buildCommand": "npm install",
        "outputDirectory": ""
      }
    ],
    
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/dist"
        }
      }
    ]
  }


  
  